%% This script visualizes data generated from the data generator
clear; close all; clc;

%% specify the path
folder = '../data/';
grid_name = 'vox';
field_name_base = 'loudness';

grid_path = [folder,grid_name];
numLoudness = 8;
for i=1:numLoudness
    field_name = [field_name_base,num2str(i-1)];
    field_path = [folder,field_name];
    [field,]
end